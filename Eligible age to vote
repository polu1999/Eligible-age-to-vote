#include<iostream>
using namespace std
int main()
{
 int age;
 cout<<"Enter Age of the user:";
 cin<<age;
 if (age<=18)
  { 
     cout<<"\nYou are eligible for voting:";
        }
     else
    {
    cout<<"You are noneligible for voting:";
    cout<<"wait for"<<18-age<<"year(s):";
     }
   getch();
       }



